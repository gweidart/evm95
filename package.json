{
  "name": "@gweidart/evm95",
  "description": "Instant retro UI for interacting with EVM compatible Smart Contracts",
  "author": "GweiDart",
  "homepage": "evm95.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gweidart/evm95.git"
  },
"publishConfig": {
  "access": "public",
  "tag": "latest",
  "registry": "https://npm.pkg.github.com"
  },
  "version": "1.0.0",
  "bin": {
    "eth95": "./dist/index.js"
  },
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "yarn tsc && yarn bundle",
    "tsc": "tsc -p . && tsc -p ./src/app",
    "bundle": "parcel build src/app/index.html -dist-dir dist --no-source-maps",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "postpublish": "npm run publish-npm",
    "publish-npm": "npm publish --access public --ignore-scripts --@cusdt-link:registry='https://registry.npmjs.org'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc",
      "pre-push": "yarn test"
    }
